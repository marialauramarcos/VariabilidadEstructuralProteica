journal = {Pure and Applied Chemistry},
keywords = {ALIGNMENT,COMPARATIVE MOLECULAR-DYNAMICS,FAMILY,FLEXIBILITY,HOMOLOGOUS,PROTEINS,SUPERFAMILY,VIBRATIONAL DYNAMICS,chemical physics,computer modeling,molecular dynamics,normal modes,protein dynamics,protein evolution},
number = {9},
