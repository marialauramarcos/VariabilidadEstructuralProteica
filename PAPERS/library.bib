isbn = {0737-4038},
issn = {15371719},
journal = {Molecular Biology and Evolution},
keywords = {Bayesian phylogenetics,Globin evolution,Statistical alignment,Stochastic processes,Structural alignment},
number = {9},
pages = {2251--2266},
pmid = {24899668},
title = {{Simultaneous Bayesian estimation of alignment and phylogeny under a joint model of protein sequence and structure}},
volume = {31},
year = {2014}
