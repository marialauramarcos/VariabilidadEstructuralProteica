journal = {Molecular Biology and Evolution},
keywords = {contact number,local packing density,protein evolution,protein structure,relative solvent accessibility,site-specific evolutionary rate,weighted contact number},
number = {1},
pages = {135--139},
pmid = {24109601},
title = {{Site-specific structural constraints on protein sequence evolutionary divergence: Local packing density versus solvent exposure}},
volume = {31},
year = {2014}
